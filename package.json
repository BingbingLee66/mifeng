    "cross-env": "^7.0.3",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^5.2.2",
    "html-webpack-plugin": "3.2.0",
    "lint-staged": "^11.1.2",
    "mockjs": "1.0.1-beta3",
    "node-sass": "^4.14.1",
    "prettier": "^2.7.1",
    "runjs": "^4.3.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "2.1.3",
    "script-loader": "0.7.2",
    "serve-static": "^1.13.2",
    "svg-sprite-loader": "4.1.3",
    "svgo": "1.2.2",
    "vue-template-compiler": "2.6.10"
