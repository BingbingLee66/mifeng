{
  "name": "linjiashop-admin",
  "version": "4.1.0",
  "description": "A  shop Admin with Element UI & axios & iconfont & permission control & lint",
  "author": "enilu<eniluzt@qq.com>",
  "license": "MIT",
  "scripts": {
    "serve": "cross-env NODE_ENV=development vue-cli-service serve",
    "local": "cross-env NODE_ENV=development vue-cli-service serve --open --mode local",
    "dev": "cross-env NODE_ENV=development vue-cli-service serve --open --mode development",
    "dev-s01a": "cross-env NODE_ENV=development vue-cli-service serve --open --mode dev-s01a",
    "dev2": "cross-env NODE_ENV=development vue-cli-service serve --open --mode development2",
    "test": "cross-env NODE_ENV=development vue-cli-service serve --open --mode test",
    "test2": "cross-env NODE_ENV=development vue-cli-service serve --open --mode test2",
    "test3": "cross-env NODE_ENV=development vue-cli-service serve --open --mode test3",
    "test4": "cross-env NODE_ENV=development vue-cli-service serve --open --mode test4",
    "test5": "cross-env NODE_ENV=development vue-cli-service serve --open --mode test5",
    "test6": "cross-env NODE_ENV=development vue-cli-service serve --open --mode test6",
    "k8s": "cross-env NODE_ENV=development vue-cli-service serve --open --mode k8s",
    "prod": "cross-env NODE_ENV=development vue-cli-service serve --open --mode production",
    "build:dev": "cross-env NODE_ENV=production vue-cli-service build --mode development",
    "build:dev-s01a": "cross-env NODE_ENV=production vue-cli-service build --mode dev-s01a",
    "build:dev2": "cross-env NODE_ENV=production vue-cli-service build --mode development2",
    "build:test": "cross-env NODE_ENV=production vue-cli-service build --mode test",
    "build:test2": "cross-env NODE_ENV=production vue-cli-service build --mode test2",
    "build:test3": "cross-env NODE_ENV=development vue-cli-service build --mode test3",
    "build:test4": "cross-env NODE_ENV=production vue-cli-service build --mode test4",
    "build:test5": "cross-env NODE_ENV=production vue-cli-service build --mode test5",
    "build:test6": "cross-env NODE_ENV=production vue-cli-service build --mode test6",
    "build:k8s": "cross-env NODE_ENV=production vue-cli-service build --mode k8s",
    "build:prod": "vue-cli-service build --mode production",
    "build:stage": "cross-env NODE_ENV=production vue-cli-service build --mode staging",
    "preview": "node build/index.js --preview",
    "lint": "eslint --ext .js,.vue src",
    "test:unit": "jest --clearCache && vue-cli-service test:unit",
    "test:ci": "npm run lint && npm run test:unit",
    "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
  },
  "dependencies": {
