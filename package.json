    "echarts": "^5.4.0",
    "element-resize-detector": "^1.2.1",
    "element-ui": "^2.15.12",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.0.0-rc.7",
    "js-cookie": "2.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "path-to-regexp": "2.4.0",
